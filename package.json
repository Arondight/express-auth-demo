{
  "name": "express-login-demo",
  "version": "0.1",
  "description": "login demo with Express.js and Vue.js",
  "repository": "https://github.com/Arondight/express-login-demo.git",
  "homepage": "https://github.com/Arondight/express-login-demo",
  "bugs": "https://github.com/Arondight/express-login-demo.git/issues",
  "author": "Qin Fandong",
  "license": "MIT",
  "type": "module",
  "exports": "./app.js",
  "keywords": [
    "express",
    "login",
    "vue"
  ],
  "engines": {
    "node": ">16.0.0"
  },
  "imports": {
    "#server/*": "./server/*.js",
    "#src/*": "./src/*.js"
  },
  "scripts": {
    "start": "node server/api.js",
    "build": "npx vite build",
    "check": "npx eslint ./*.js ./*.html ./server/ ./src/",
    "format": "npx prettier --config ./.prettierrc.json -uw ./*.md ./*.js ./.*.json ./*.json ./*.html ./server/ ./src/",
    "update": "npx ncu -u; npm update"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "2.3.0",
    "express": "4.17.3",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "mongoose": "6.2.10",
    "vite": "2.9.0",
    "vue": "3.2.25",
    "yargs": "17.4.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "eslint": "8.12.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-sort-exports": "0.6.0",
    "eslint-plugin-vue": "8.5.0",
    "eslint-plugin-yml": "0.14.0",
    "npm-check-updates": "12.5.7",
    "prettier": "2.6.2"
  }
}
